    "import numpy as np\n",
    "X = np.array([[-1, -1], [-2, -1], [1, 1], [2, 1]])\n",
    "y = np.array([3, 3, 4, 4])\n",
    "from sklearn.svm import SVC\n",
    "clf = SVC()\n",
    "clf.fit(X, y) \n",
    "\n",
    "\n",
    "\n",
    "\n",
    "print(clf.predict([[1, -2]]))\n"
   ]
  },
